<!DOCTYPE html>
<html>
  <head>
    <title>Schedule Me This</title>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
    <!-- Include link to the Materialize Icons -->
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <%= csrf_meta_tags %>
  </head>
  <body>
    <div class="container" id="main_container">
      <!-- Page Content goes here -->
      <div class="row">
	<div class="col s12 m12 l12"><%= render :partial => "partials/header" %></div>
      </div>
      <div class="row">
	<div class="col s12 m12 l12" id="main_content"><%= yield %></div>
      </div>
      <div class="col s12 m12 l12"><%= render :partial => "partials/footer" %></div>
    </div>
  </body>
</html>
